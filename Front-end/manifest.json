{
    "name": "Streaming Desktop",
    "description": "Streaming Desktop",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
      "default_title": "Streaming Desktop",
      "default_icon": "images/record16.png",
      "icons": { 
        "16": "images/record16.png",
        "32": "images/record32.png",
        "64": "images/record64.png",
        "128": "images/record128.png" 
      },
      "default_popup": "index.html"
    },
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.socket.io/4.5.0/socket.io.min.js; object-src 'self'",
    "permissions": [
      "tabs",
      "https://cdn.socket.io/4.5.0/socket.io.min.js",
      "<all_urls>"
    ]
}
